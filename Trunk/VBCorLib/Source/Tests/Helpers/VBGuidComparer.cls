VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
  Persistable = 0  'NotPersistable
  DataBindingBehavior = 0  'vbNone
  DataSourceBehavior  = 0  'vbNone
  MTSTransactionMode  = 0  'NotAnMTSObject
END
Attribute VB_Name = "VBGuidComparer"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Explicit
Implements SimplyVBUnitType.IComparer

Private Function IComparer_Compare(X As Variant, Y As Variant) As Long
    Dim XGuid As VBGUID
    Dim YGuid As VBGUID
    
    XGuid = X
    YGuid = Y
    
    If XGuid.Data1 < YGuid.Data1 Then
        IComparer_Compare = -1
    ElseIf XGuid.Data1 > YGuid.Data1 Then
        IComparer_Compare = 1
    End If
End Function
